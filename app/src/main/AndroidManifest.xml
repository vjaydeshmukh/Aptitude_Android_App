<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aptitudetestquiz">

    <application
        android:allowBackup="true"
        android:label="Anywhere Aptitude"
        android:supportsRtl="true"
        android:icon="@mipmap/ic_launcher1"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Solution"></activity>
        <activity android:name=".SignupActivity" />
        <activity android:name=".DomainOptions" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".questionspage"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".instructionpage" />
        <activity android:name=".levels" />
        <activity android:name=".MainActivity" />
        <activity android:name=".score" />
        <activity android:name=".AndroidDatabaseManager" />
        <activity android:name=".splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>